{
  "name": "hydrazine",
  "version": "0.0.1",
  "description": "Redux-Little-Router apps from concentrate.",
  "main": "dist/index.js",
  "scripts": {
    "build:start": "mkdir build-tmp || echo 'build-tmp already exists'",
    "build:babel": "babel src -d build-tmp",
    "build:generators": "regenerator --include-runtime build-tmp dist",
    "build:cleanup": "rm -rf build-tmp",
    "build": "npm run build:start && npm run build:babel && npm run build:generators && npm run build:cleanup",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --progress --config example/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scholarship-junkies/hydrazine.git"
  },
  "keywords": [
    "router",
    "redux",
    "redux-saga"
  ],
  "author": "Brent Schroeter <brentsch@uw.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scholarship-junkies/hydrazine/issues"
  },
  "homepage": "https://github.com/scholarship-junkies/hydrazine#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.13.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "regenerator": "^0.8.46"
  },
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-little-router": "^8.1.0",
    "redux-saga": "^0.11.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  }
}
